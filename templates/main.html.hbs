<!DOCTYPE html>
<html lang={{lang}}>
    <head>
        <title>{{title}}</title>
        <meta name="viewport" content="width=device-width, initial-scale=1">
        <meta charset="utf-8">
{{#each css as |file|}}
        <link rel="stylesheet" href="{{file}}">
{{/each}}
{{#each js as |file|}}
        <script src="{{file}}"></script>
{{/each}}
    </head>
    <body>
        {{> page}}
    </body>
</html>
